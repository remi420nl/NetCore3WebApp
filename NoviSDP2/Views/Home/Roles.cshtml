@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>

@{ ViewData["Title"] = "Index"; }

<h1> Index </h1>

<table>
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
            </tr>

        }
    </tbody>
</table>